<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>ClockAnalyser</id>
    <version>1.0.0.9</version>
    <title>ClockAnalyser</title>
    <authors>Michal Motyčka</authors>
    <owners>Michal Motyčka</owners>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/Melchy/Clock</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Analyser for Clock package.</description>
    <releaseNotes> </releaseNotes>
    <copyright>Copyright</copyright>
    <tags>DateTimeProvider, Clock, Analyser</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>